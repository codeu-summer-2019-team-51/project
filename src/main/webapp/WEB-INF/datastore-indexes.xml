<?xml version="1.0" encoding="utf-8"?>
<datastore-indexes
  autoGenerate="true">
  <datastore-index kind="Thread" ancestor="false" source="manual">
      <property name="communityId" direction="asc"/>
      <property name="name" direction="asc"/>
  </datastore-index>

  <datastore-index kind="Comment" ancestor="false" source="manual">
      <property name="threadId" direction="asc"/>
      <property name="timestamp" direction="desc"/>
  </datastore-index>

  <datastore-index kind="UserBook" ancestor="false" source="manual">
      <property name="user" direction="asc"/>
      <property name="timestamp" direction="desc"/>
  </datastore-index>
</datastore-indexes>
